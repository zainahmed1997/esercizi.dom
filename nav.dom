<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>

  <body>
    <ul>
      <li>1</li>
      <li>2</li>
      <li>3</li>
    </ul>
    <script>
      const html = document.documentElement;
      const body = html.children[1];
      const ul = body.children[0];
      const li = ul.children[1];
      const li1 = li.previousElementSibling;
      const li2 = li.nextElementSibling;
      console.log(html, body, ul, li, li1, li2);
    </script>
  </body>
</html>
